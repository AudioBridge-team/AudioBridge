{
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.DS_Store": true,
        "**/.factorypath": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.svn": true,
        "**/CVS": true,
    },
    "keep-a-changelog.defaultAuthor": "AudioBridge-team",
    "keep-a-changelog.defaultRepository": "AudioBridge",
    "keep-a-changelog.dateFormat": "DD/MM/YYYY",
    "keep-a-changelog.validFiles": [
        "changelog"
    ],
    "autoDocstring.docstringFormat": "google",
    "files.trimFinalNewlines": true,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
}
